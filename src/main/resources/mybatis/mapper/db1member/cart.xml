<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mycompany.webapp.dao.db1member.CartDao">
	<!-- cart delete -->
	<delete id="deleteByMemberIdAndProductStockId" parameterType="com.mycompany.webapp.vo.Cart">
	delete from cart 
	where mid = #{mid} 
	and pstockid = #{pstockid}
	</delete>
</mapper>